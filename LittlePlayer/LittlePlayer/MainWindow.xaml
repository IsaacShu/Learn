<Window x:Class="LittlePlayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LittlePlayer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="225">
    <Grid HorizontalAlignment="Right" Width="215" Margin="0,0,2,0">
        <Button x:Name="Mp3" Content="Mp3播放器" HorizontalAlignment="Left" Height="39" Margin="30,35,0,0" VerticalAlignment="Top" Width="156" Click="Mp3_Click"/>
        <Button x:Name="Flash" Content="Flash播放器" HorizontalAlignment="Left" Height="39" Margin="30,150,0,0" VerticalAlignment="Top" Width="156" RenderTransformOrigin="0.512,2.211"/>

    </Grid>
</Window>
